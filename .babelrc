{
  "presets": [
    [
      "next/babel",
      {
        "styled-jsx": {
          "plugins": ["styled-jsx-plugin-sass"]
        }
      }
    ]
  ],
  "plugins": [
    [
      "styled-components",
      {
        "ssr": false,
        "displayName": true,
        "preprocess": false
      }
    ],
    [
      "styless",
      {
        "import": "~antd/lib/style/themes/default.less",
        "lessOptions": {
          "javascriptEnabled": true
        }
      }
    ]
  ]
}
